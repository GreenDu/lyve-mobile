{
  "name": "lyve-mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo run:android",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "ios": "expo run:ios",
    "start": "expo start",
    "web": "expo start --web",
    "android:native": "expo run:android -d --no-build-cache",
    "ios:native": "expo run:ios -d --no-build-cache",
    "test": "jest --watch --coverage=false --changedSince=origin/main",
    "testDebug": "jest -o --watch --coverage=false",
    "testFinal": "jest",
    "coverage": "jest -i --coverage --passWithNoTests",
    "install-maestro": "curl -Ls 'https://get.maestro.mobile.dev' | bash",
    "test:e2e": "maestro test .maestro/ -e APP_ID=com.anonymous.lyve-mobile"
  },
  "dependencies": {
    "@config-plugins/react-native-webrtc": "^9.0.0",
    "@expo/react-native-action-sheet": "^4.1.0",
    "@expo/vector-icons": "^14.0.0",
    "@giphy/react-native-sdk": "^3.2.6",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-navigation/native": "^6.1.7",
    "@shopify/flash-list": "1.6.3",
    "@tamagui/font-inter": "1.74.8",
    "@tamagui/react-native-media-driver": "1.74.8",
    "@tamagui/shorthands": "1.74.8",
    "@tamagui/themes": "1.74.8",
    "@tanstack/react-query": "^5.35.5",
    "axios": "^1.6.8",
    "base-64": "^1.0.0",
    "expo": "~50.0.17",
    "expo-asset": "~9.0.2",
    "expo-auth-session": "~5.4.0",
    "expo-camera": "~14.1.3",
    "expo-constants": "~15.4.5",
    "expo-crypto": "~12.8.1",
    "expo-dev-client": "~3.3.11",
    "expo-font": "~11.10.2",
    "expo-image": "~1.10.6",
    "expo-image-picker": "~14.7.1",
    "expo-linking": "~6.2.2",
    "expo-router": "~3.4.10",
    "expo-splash-screen": "~0.26.5",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "expo-web-browser": "~12.8.2",
    "formik": "^2.4.6",
    "install": "^0.13.0",
    "jwt-decode": "^4.0.0",
    "mediasoup-client": "^3.7.8",
    "moment": "^2.30.1",
    "npx": "^10.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-incall-manager": "^4.2.0",
    "react-native-modal": "^13.0.1",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0",
    "react-native-toast-message": "^2.2.0",
    "react-native-web": "~0.19.10",
    "react-native-webrtc": "^118.0.7",
    "react-query-kit": "^3.3.0",
    "react-test-renderer": "18.2.0",
    "socket.io-client": "^4.7.5",
    "tamagui": "1.74.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@faker-js/faker": "^8.4.1",
    "@tamagui/babel-plugin": "1.74.8",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.5.1",
    "@types/base-64": "^1.0.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.14",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.1",
    "jest": "^29.7.0",
    "jest-expo": "50",
    "prettier": "^3.2.5",
    "socket.io-mock": "^1.3.2",
    "typescript": "^5.4.5"
  },
  "jest": {
    "preset": "jest-expo",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/**/*.{js,jsx,ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ],
    "modulePathIgnorePatterns": [
      "./src/app"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "moduleFileExtensions": [
      "js",
      "mjs",
      "cjs",
      "jsx",
      "ts",
      "tsx",
      "json",
      "node"
    ]
  },
  "eslintConfig": {
    "extends": "universe/native"
  },
  "resolutions": {
    "expo-modules-core": "~1.11.0"
  },
  "overrides": {
    "expo-modules-core": "~1.11.0"
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native-safe-area-context"
      ]
    }
  },
  "private": true
}
